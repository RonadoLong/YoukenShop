/* pages/profile/profile.wxss */
.container{
  padding:0 0 1rem 0;
}
.myheader{
  background:#FFCF61;
  width:100%;
  height:140px;
  margin-bottom:.5rem;
}
.userPhoto{
  display:inline-block;
  width:28%;
}
.myheader .photo{
  width:3.5rem;
  height:3.5rem;
  line-height: 3.5rem;
  background-color:#ffffff;
  border-radius:100%;
  margin-top:1.5rem;
  margin-left:1.5rem;
  text-align:center;
  border:1px solid #ffffff;
}
.myheader .photo image{
  border-radius: 100%;
  width:3.5rem;
  height:3.5rem;
  background-size:3.5rem 3.5rem;
  background-repeat:no-repeat;
}
.userName{
  width:55%;
  display: inline-block;
  position:absolute;
  top:2.3rem;
  margin-left:.3rem;

}
.userName span{
  display:inline-block;
}
.userName .name{
  font-size:.8rem;
  color:#444444;
}
.leftIocn{
  width:1rem;
  height:1rem;
  position: absolute;
  left:1rem;
}
.leftIocn image{
  background-size:1rem;
  background-repeat:no-repeat;
}
van-cell-group{
  width:100%;
  position: relative;
}
.cell-index--van-cell__left-icon{
  color:#fac685;
  font-size:.9rem;
}
.van-cell {
  border-bottom:1px solid #eaeaea;
}
.returnBack{
  width:100%;
  background: #ffffff;
  text-align: center;
  padding:.5rem;
  font-size: .8rem;
  margin-top:1rem;
}
.wux-cell{
  font-size:.75rem !important;
}